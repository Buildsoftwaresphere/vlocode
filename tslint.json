{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": false,
		"typeof-compare": true,
		"use-isnan": true,
		"deprecation": true,
		"prefer-readonly": true
	},
	"defaultSeverity": "warning"
}